<!DOCTYPE html>
<html>

<head>
    <title>BMS - Asset</title>
    <% include ../partial/head.ejs %>
    <script>
        var appliance = '<%- appliance_id %>';
    </script>

    <script src="/js/asset/asset.js"></script>

</head>

<body ng-app="asset" class="hold-transition skin-purple fixed sidebar-mini" ng-cloak>

    <div class="wrapper">
        <% include ../partial/headbar.ejs %>
        <% include ../partial/sidebar.ejs %>
        <div class="content-wrapper">
            <section class="content-header">
                <h1>
                    Asset
                </h1>
                <ol class="breadcrumb">
                    <li><a href="/"><i class="fa fa-home"></i>Home</a></li>
                    <li class="active">
                        Asset
                    </li>
                </ol>
            </section>
            <section class="content" ng-controller="assetController">
                <div class="row">
                    <div class="col-md-12">
                        <div class="box box-primary" ng-cloak>
                            <div class="box-header with-border" data-widget="collapse">
                                <h3 class="box-title">Device Information</h3>
                            </div>
                            <!-- /.box-header -->

                            <div class="box-body">
                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="table-responsive">
                                            <table id="table-selected-device" class="table table-condensed table-hover"
                                                style="width: 100%;">
                                            </table>
                                        </div>
                                    </div>

                                </div>
                                <!-- /.row -->
                            </div>
                            <!-- /.box-body -->
                        </div>
                    </div>

                    <div class="col-md-6">
                        <%- include ../asset/side_left.ejs %>


                    </div>
                    <div class="col-md-6">
                        <%- include ../asset/side_right.ejs %>

                    </div>
                    <%- include ../asset/asset_description_table.ejs %>
                </div>
            </section>
        </div>
        <%- include ../partial/footer.ejs %>
    </div>
</body>

</html>