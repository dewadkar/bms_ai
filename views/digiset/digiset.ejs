<!DOCTYPE html>
<html>

<head>
    <title>BMS - DG Set</title>
    <% include ../partial/head.ejs %>
    <script src="/js/digiset/digiset.js"></script>
</head>

<body ng-app="digiset" class="hold-transition skin-purple fixed sidebar-mini" ng-cloak>

    <div class="wrapper">
        <% include ../partial/headbar.ejs %>
        <% include ../partial/sidebar.ejs %>
        <div class="content-wrapper">
            <section class="content-header">
                <h1>
                    DG set
                </h1>
                <ol class="breadcrumb">
                    <li><a href="/digiset"><i class="fa fa-home"></i>DG set</a></li>
                </ol>
            </section>

            <section class="content" ng-controller="digisetController">
                <div class="row">
                    <div class="col-md-8">
                        <div class="box box-primary" ng-cloak>

                            <div class="box-body">
                                <div class="row">

                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="box box-primary" ng-cloak>

                            <div class="box-body">
                                <div class="row">

                                    ROW
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-9">
                        <div class="row">
                            <div class="col-md-12">
                                <div class="box box-primary" ng-cloak>

                                    <div class="box-body">
                                        <div class="row">
                                            <div class="col-md-4">
                                                <canvas id="run_hours" width="400" height="400" aria-label="Hello ARIA World" role="img"></canvas>
                                            </div>
                                            <div class="col-md-4">
                                                <canvas id="energy_generated" width="400" height="400" aria-label="Hello ARIA World" role="img"></canvas>
                                            </div>
                                            <div class="col-md-4">
                                                <canvas id="fuel_used" width="400" height="400" aria-label="Hello ARIA World" role="img"></canvas>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="col-md-9">
                                        <div class="box box-primary" ng-cloak>

                                            <div class="box-body">
                                                <div class="row">
                                                    <div class="col-md-12">
                                                        <canvas id="generator_fuels_used" width="900" height="400" aria-label="Hello ARIA World" role="img"></canvas>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-9">
                                        <div class="box box-primary" ng-cloak>

                                            <div class="box-body">
                                                <div class="row">
                                                    <canvas id="fuel_filled" width="900" height="400" aria-label="Hello ARIA World" role="img"></canvas>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </div>

                    <div class="col-md-3">
                        <div class="row">
                            <div class="col-md-12">
                                <div class="box box-primary" ng-cloak>

                                    <div class="box-body">
                                        <div class="row">
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-12">
                                <div class="box box-primary" ng-cloak>

                                    <div class="box-body">
                                        <div class="row">
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-12">
                                <div class="box box-primary" ng-cloak>

                                    <div class="box-body">
                                        <div class="row">
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>



                </div>
            </section>
        </div>
        <%- include ../partial/footer.ejs %>
    </div>
</body>

</html>
